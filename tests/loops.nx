fun main() = (
    var i = 0;
    while i < 10 do (
        printi i;
        break;
        i = i + 1
    );

    var i = 0;
    while i < 10 do (
        printi i;
        if i == 5 then
            break;
        i = i + 1
    );

    var i = 0;
    var iter_end = 10;
    if i < iter_end then
        while true do (
            printi i;
            if i < iter_end then
                i = i + 1
            else
                break
        );

    for i = 0 to 10 do
        printi i
)
